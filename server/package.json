{
  "main": "dist",
  "scripts": {
    "dev": "nodemon --inspect -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "license": "MIT",
  "dependencies": {
    "acl": "^0.4.11",
    "async": "^2.6.0",
    "babel-runtime": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "bignumber.js": "^7.0.1",
    "body-parser": "^1.13.3",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "dotenv": "^5.0.1",
    "ethereumjs-block": "^1.7.1",
    "ethereumjs-units": "^0.2.0",
    "ethereumjs-util": "^5.1.5",
    "express": "^4.13.3",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.0.15",
    "morgan": "^1.8.0",
    "npm": "^6.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "socket.io": "^2.1.0",
    "web3": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
